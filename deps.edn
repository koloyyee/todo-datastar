{:paths ["src" "resources"]
 :deps {org.clojure/clojure {:mvn/version "1.11.1"}
        ;; datastar setup
        dev.data-star.clojure/sdk {:mvn/version "1.0.0-RC3"}
        dev.data-star.clojure/malli-schemas {:mvn/version "1.0.0-RC3"}
        ;; http-kit
        http-kit/http-kit              {:mvn/version "2.8.1"}
        dev.data-star.clojure/http-kit {:mvn/version "1.0.0-RC3"}
        dev.data-star.clojure/http-kit-malli-schemas {:mvn/version "1.0.0-RC3"}
        ;; ring
        ring/ring-jetty-adapter    {:mvn/version "1.13.0"}
        dev.data-star.clojure/ring {:mvn/version "1.0.0-RC4"}
        dev.data-star.clojure/ring-malli-schemas {:mvn/version "1.0.0-RC4"}
        ;; rendering
        com.cnuernber/charred {:mvn/version "1.037"}
        dev.onionpancakes/chassis {:mvn/version "1.0.365"}
        hiccup/hiccup {:mvn/version "2.0.0"}
        ;; routing
        metosin/reitit {:mvn/version "0.9.1"}
        ;; AI
        net.clojars.wkok/openai-clojure {:mvn/version "0.23.0"}
        ;; Database
        com.zaxxer/HikariCP {:mvn/version "6.3.0"}
        org.postgresql/postgresql {:mvn/version "42.7.7"}
        com.github.seancorfield/next.jdbc {:mvn/version "1.3.1070"}
        org.flywaydb/flyway-core {:mvn/version "11.8.2"}
        org.flywaydb/flyway-database-postgresql {:mvn/version "11.8.2"}
        com.github.seancorfield/honeysql {:mvn/version "2.7.1350"}
        com.datomic/peer {:mvn/version "1.0.7387"}}
 :aliases
 {:run-m {:main-opts ["-m" "todo-datastar.main"]}
  :run-x {:ns-default todo-datastar.main
          :exec-fn greet
          :exec-args {:name "Clojure"}}
  :build {:deps {io.github.clojure/tools.build
                 {:git/tag "v0.9.2" :git/sha "fe6b140"}}
          :ns-default build}
  :test {:extra-paths ["test"]
         :extra-deps {org.clojure/test.check {:mvn/version "1.1.1"}
                      io.github.cognitect-labs/test-runner
                      {:git/tag "v0.5.1" :git/sha "dfb30dd"}}}
  :neil {:project {:name todo-datastar/todo-datastar}}}}
